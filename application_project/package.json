    "build:safe": "node build-safe.js",
    "build:final": "node build-final.js",
    "build:simple": "node build-simple.js",
    "build:android": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:prod": "eas build --platform ios --profile production",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "crypto-js": "^4.2.0",
    "expo": "^54.0.10",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "^8.0.9",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-network": "^8.0.7",
    "expo-router": "~6.0.8",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
